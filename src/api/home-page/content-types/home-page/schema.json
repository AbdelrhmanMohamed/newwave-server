      "repeatable": true,
      "component": "homepage.key-services"
    },
    "projects_headline": {
      "type": "string",
      "default": "Portfolio"
    },
    "projects_title": {
      "type": "string",
      "default": "Our Project Portfolio"
    },
    "projects_description": {
      "type": "text"
    },
    "partners": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::partner.partner"
    }
  }
}
